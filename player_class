class Player(): 
    
    def __init__(self, x, y, width, height):
        
        self.x = x 
        self.y = y 
        self.width = width 
        self.height = height 
        
        self.jumpHeight = 10
        self.velocity = 5
        self.walkCount = 0
        
        self.jump = False 
        self.left = False
        self.right = False 


